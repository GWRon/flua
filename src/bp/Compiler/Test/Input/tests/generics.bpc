myArray = ElementHolder<Int>()
myArray.value = 5

gh = ElementHolder<ElementHolder<Int>>()
gh.value = myArray
f = gh.value

myArray.value = 7
f = gh.value

if f.value == 7
	print "Generics are working!"

if gh.value == myArray
	print "Generics are truly working!"

ElementHolder
	template
		TDataType
	
	init
		self.value : TDataType
		
	get
		value
			return self.value
		
	set
		value element
			self.value = element