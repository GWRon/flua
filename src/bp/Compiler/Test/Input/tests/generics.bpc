sample = ElementHolder<Int>()
sample.value = 5				# Int

wrapper = ElementHolder<ElementHolder<Int>>()
wrapper.value = sample			# ElementHolder<Int>
inst = wrapper.value			# ElementHolder<Int>

sample.value = 7				# Int

if inst.value == 7				# Int
	print "Generics are working!"

if wrapper.value == sample		# ElementHolder<Int>
	print "Generics are truly working!"
	
if wrapper == wrapper		# ElementHolder<ElementHolder<Int>>
	print "Generics are perfectly working!"

ElementHolder
	template
		TDataType
	
	init
		self.value : TDataType
		
	get
		value
			self.a = 5
			return self.value
		
	set
		value element
			self.value = element