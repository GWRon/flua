Point
	template
		Tx = Float
		Ty = Float
		
	init self.x : Tx, self.y : Ty
		...
		
	move offX, offY
		self.x += offX
		self.y += offY
		
	get
		x
			return self.x
			
		y
			return self.y
	
	set
		x value
			self.x = value
		
		y value
			self.y = value
			
	distanceTo point
		return distance(self.x, self.y, point.x, point.y)

# Overloaded functions
print p : Point
	bp_print p.x
	bp_print ", "
	bp_print p.y
	bp_print "\n"