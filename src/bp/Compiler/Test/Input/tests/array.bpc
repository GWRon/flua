import bp.Collection.Array

errorCode = 0

array = Array<Int>()

if array.length != 0
	errorCode += 1

array.add(5)
array.add(8)
array.add(13)

if array[0] + array[1] != array[2]
	errorCode += 1

if array.length != 3
	errorCode += 1

array2 = Array<Array<Int>>()
array2.add(array)

if array[0] != array2[0][0] or array[1] != array2[0][1] or array[2] != array2[0][2]
	errorCode += 1

if errorCode
	print "Array module NOT working!"
else
	print "Array module working!"